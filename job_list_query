jobs related or calls the stored procedure

SELECT  
    j.name AS JobName,
    s.step_id,
    s.step_name,
    s.subsystem,
    s.command
FROM dbo.sysjobs j
JOIN dbo.sysjobsteps s
    ON j.job_id = s.job_id
WHERE s.command LIKE '%YourStoredProcedureName%'
ORDER BY j.name, s.step_id;
USE msdb;
GO



Dependency check

SELECT  
    OBJECT_SCHEMA_NAME(d.referencing_id) AS SchemaName,
    OBJECT_NAME(d.referencing_id) AS ObjectName,
    o.type_desc,
	    d.referenced_entity_name AS entityReferenced

FROM sys.sql_expression_dependencies d
JOIN sys.objects o
    ON d.referencing_id = o.object_id

